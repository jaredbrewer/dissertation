@article{Chang2013,
	abstract = {BACKGROUND: Cachexia is a hallmark of pulmonary tuberculosis and is associated with poor prognosis. A better understanding of the mechanisms behind such weight loss could reveal targets for therapeutic intervention. The role of appetite-regulatory hormones in tuberculosis is unknown. METHODS AND FINDINGS: 41 subjects with newly-diagnosed pulmonary TB (cases) were compared to 82 healthy controls. We measured appetite, body mass index (BMI), % body fat (BF), plasma peptide YY (PYY), leptin, ghrelin, and resistin for all subjects. Measurements were taken at baseline for controls and at treatment days 0, 30, and 60 for cases. Baseline appetite, BMI, and BF were lower in cases than in controls and improved during treatment. PYY, ghrelin, and resistin were significantly elevated in cases and fell during treatment. Leptin was lower in cases and rose with treatment. Appetite was inversely related to PYY in cases. High pre-treatment PYY predicted reduced gains in appetite and BF. PYY was the strongest independent predictor of appetite in cases across all time points. CONCLUSIONS: Appetite-regulatory hormones are altered in TB patients. As hormones normalize during treatment, appetite is restored and nutritional status improves. High baseline PYY is an indicator of poor prognosis for improvement in appetite and nutrition during treatment. Wasting in TB patients may partly be mediated by upregulation of PYY with resulting appetite suppression.},
	author = {Chang, S. W. and Pan, W. S. and Lozano Beltran, D. and Oleyda Baldelomar, L. and Solano, M. A. and Tuero, I. and Friedland, J. S. and Torrico, F. and Gilman, R. H.},
	date-added = {2022-10-17 12:01:12 -0400},
	date-modified = {2022-10-17 12:04:56 -0400},
	doi = {10.1371/journal.pone.0054564},
	issn = {1932-6203 (Electronic) 1932-6203 (Linking)},
	journal = {PLoS One},
	keywords = {Appetite/*physiology Cachexia/*physiopathology Cohort Studies Gastrointestinal Hormones/*physiology Humans Nutritional Status Tuberculosis, Pulmonary/*physiopathology},
	note = {Chang, Suzanne W Pan, William S Lozano Beltran, Daniel Oleyda Baldelomar, Lizet Solano, Marco Antonio Tuero, Iskra Friedland, Jon S Torrico, Faustino Gilman, Robert H eng MR/K007467/1/MRC_/Medical Research Council/United Kingdom T35 AI065385/AI/NIAID NIH HHS/ AI065385/AI/NIAID NIH HHS/ Research Support, N.I.H., Extramural Research Support, Non-U.S. Gov't 2013/01/30 PLoS One. 2013;8(1):e54564. doi: 10.1371/journal.pone.0054564. Epub 2013 Jan 24.},
	number = {1},
	pages = {e54564},
	title = {Gut hormones, appetite suppression and cachexia in patients with pulmonary TB},
	type = {Journal Article},
	url = {https://www.ncbi.nlm.nih.gov/pubmed/23358528},
	volume = {8},
	year = {2013},
